// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./tennis_club.db"
}

model Player {
  id    Int     @id @default(autoincrement())
  first_name  String
  last_name  String
  nationality  String
  dob  Int
  score  Int
}
